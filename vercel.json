{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/serverless.js"
    }
  ],
  "routes": [
    {
      "src": "/api/*",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "headers": "Origin, X-Requested-With, Content-Type, Accept",
      "dest": "/api/:path*",
      "cors": {
        "origin": "https://barrylinx.github.io",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
        ],
        "allowedHeaders": [
          "Content-Type",
          "Authorization"
        ]
      }
    }
  ]
}